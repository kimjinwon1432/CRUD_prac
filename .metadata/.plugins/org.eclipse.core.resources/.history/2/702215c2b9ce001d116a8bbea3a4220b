		VALUES (#id#,#name#,#phone#,#email#,#address#,#postNum#)
	</insert>	
	<insert id="crudDAO.insertLoginInfo">
		INSERT INTO logininfo(id, password)
		VALUES (#id#,#password#)
	</insert>
	
	<insert id="crudDAO.insertAgreement">
		INSERT INTO agreement(id)
		VALUES (#id#)
	</insert>

