	}

	public String insertAgreement(CrudVO vo) {
		// TODO Auto-generated method stub
		return (String) insert("crudDAO.insertAgreement", vo);
	}

	public int checkId(String id) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	
	
}
